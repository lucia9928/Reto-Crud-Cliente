<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="737.0" prefWidth="1121.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eus.tartangalh.crud.controladores.ProveedorFXMLController">
   <children>
      <!-- TableView con fx:id -->
      <TableView fx:id="tableView" layoutX="161.0" layoutY="145.0" prefHeight="447.0" prefWidth="800.0">
        <columns>
          <!-- Columnas con fx:id -->
          <TableColumn fx:id="idProveedorColumna" prefWidth="101.0" text="idProveedor" />
          <TableColumn fx:id="calleColumna" prefWidth="95.0" text="calle" />
          <TableColumn fx:id="cifColumna" prefWidth="93.0" text="cif" />
          <TableColumn fx:id="ciudadColumna" minWidth="0.0" prefWidth="119.0" text="ciudad" />
          <TableColumn fx:id="codPostalColumna" prefWidth="100.0" text="codPostal" />
          <TableColumn fx:id="fechaContratacionColumna" prefWidth="152.0" text="fechaContratacion" />
          <TableColumn fx:id="nombreProveedorColumna" prefWidth="139.0" text="nombreProveedor" />
        </columns>
      </TableView>

      <!-- TextField con fx:id -->
      <TextField fx:id="searchField" layoutX="812.0" layoutY="60.0" />
      
      <!-- Botón con fx:id -->
      <Button fx:id="searchButton" layoutX="990.0" layoutY="60.0" mnemonicParsing="false" text="Buscar" />
      
      <!-- ComboBox con fx:id -->
      <ComboBox fx:id="filterComboBox" layoutX="561.0" layoutY="60.0" prefWidth="150.0" />
      
      <!-- TextField adicional con fx:id -->
      <TextField fx:id="nameField" layoutX="258.0" layoutY="60.0" />
      
      <!-- Botón para crear fila con fx:id -->
      <Button fx:id="createRowButton" layoutX="161.0" layoutY="644.0" mnemonicParsing="false" text="Crear fila" />
   </children>
</AnchorPane>
